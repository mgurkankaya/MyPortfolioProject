@model PortfolioProject.Models.TlbProjects
@{
    ViewBag.Title = "AddProject";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h3 class="mt-3">Add Project Page</h3>

<form method="post">
    <label>Proje Adı</label>
    <input type="text" name="ProjectName" class="form-control" /> <br />
    

    <label>Resim Url</label>
    <input type="text" name="ImageUrl" class="form-control" /> <br />
   

    <label>Github Linki</label>
    <input type="text" name="GithubUrl" class="form-control" /> <br />
   

    <label>Kategori</label>
    @Html.DropDownListFor(x=>x.CategoryId,(List<SelectListItem>)ViewBag.category,"Seç", new {@class="form-control"})


<button class="btn btn-outline-danger">Kaydet</button>
</form>